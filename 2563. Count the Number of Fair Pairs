class Solution {
public:
    long long countFairPairs(vector<int>& nums, int lower, int upper) {
        
        sort(nums.begin(),nums.end());
        int n = nums.size();
        long long ans = 0;
        for(int i=0;i<n;i++){
                int t = upper-nums[i];
                auto indxr = upper_bound(nums.begin()+i+1,nums.end(),t);
                auto indxl = lower_bound(nums.begin()+i+1,nums.end(),lower-nums[i]);
              
                ans += (indxr-indxl);
                cout<<ans<<" ";
        }
        return ans;
    }
};
